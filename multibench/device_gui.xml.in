		<!-- доступные для регулирования пределы -->
		<Limits>
		</Limits>
		<!--  калибровка -->
		<CalibrationKoefs>
		</CalibrationKoefs>
		<!-- Сделать бы так, чтобы при отсутствии параметра кнопка в меню не появлялась-->
		<!-- список выводимых параметров.
    регистр необходимо соблюдать
    (deg) - в проге заменяется на символ градуса
    Для температуры надо ставить флаг isTemperature=\"1\", для остальных 0 или отсутствие флага
    параметры без value (установленный параметр) - выводятся в текстовом виде в окне параметров и только там.
    real - измеренный параметр
    Параметры с value \"участвуют\" в hide controls.
     -->
		<ParamControls>
			<Param min=\"0020\"
     				max=\"0021\"
     				value=\"0006\">Frequency</Param>
			<Param min=\"0022\"
     				max=\"0023\"
     				value=\"0007\">Duration</Param>
			<Param min=\"0024\"
     				max=\"0025\"
     				value=\"0008\"
                                real=\"0040\"
                                fixed=\"\">Current</Param>
			<Param real=\"0003\">Serial Number</Param>
			<Param real=\"0041\">Voltage</Param>
		</ParamControls>
		<!--  список checkbox (Галочек) Имеют code - номер команды два значения при нажатии и отщёлкивании галочки.
    Могут также программироваться по ID лично в проге, тогда остальные поля оставлять пустыми,
    хотя можно попробовтаь комбинировать, но насколько синхронно это будет работать есть сомнения -->
		<Checkboxes>
			<CheckBox code=\"0004\"
        				onCommand=\"0020\"
        				offCommand=\"0040\"
        				mask=\"0004\">Digital Current Set</CheckBox>
			<CheckBox code=\"0004\"
        				onCommand=\"0400\"
        				offCommand=\"0200\"
        				mask=\"0010\">Digital Enable</CheckBox>
			<CheckBox code=\"0004\"
        				onCommand=\"2000\"
        				offCommand=\"1000\"
        				mask=\"0080\">Ignore IntLock</CheckBox>
		</Checkboxes>
		<!-- кнопочки доступные в программе. Аналогично чекбоксам, только тут чисто по протоколу -->
		<Buttons>
			<Button
      				mask=\"0002\"
      				code=\"0004\"
      				onCommand=\"0008\"
      				offCommand=\"0010\">Laser</Button>
		</Buttons>
		<Leds>
			<Led label=\"Laser\">
				<LedMask code=\"0004\"
       					mask=\"0002\"
       					maskColor=\"#00ff00\"
       					defaultColor=\"#171717\"/>
			</Led>
			<Led label=\"IntLock\">
				<LedMask code=\"0005\"
       					mask=\"0002\"
       					maskColor=\"#ffff00\">Interlock</LedMask>
			</Led>
			<Led label=\"Error\">
				<LedMask code=\"0005\"
       					mask=\"0010\"
       					maskColor=\"#ffff00\">Overheat warning</LedMask>
				<LedMask code=\"0005\"
       					mask=\"0008\"
       					maskColor=\"#ff0000\">Over current or Overheat</LedMask>
			</Led>
		</Leds>
	</Device>
